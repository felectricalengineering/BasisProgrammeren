/*

#include <stdio.h>
#include <stdlib.h>

int eerste_getal;
int tweede_getal;
int quotient;
int rest;
int product;

int main(){

printf("We gaan een deling doen waar de grootste gemene deler en de rest te zien is (alleen gehele getallen).\n\n");
printf("Kies een eerste getal.\n");
scanf("&d", &eerste_getal);

printf("\n\nKies een tweede getal.\n");
scanf("%d", &tweede_getal);

quotient = eerste_getal / tweede_getal;
product = eerste_getal * tweede_getal;
rest = eerste_getal - (tweede_getal * scanf("%.0d", &quotient));

printf("\n\nDe grootste gemene deler is %.0d.", &quotient);
printf("\n\nDe rest is %d.", &rest);
}

*/

