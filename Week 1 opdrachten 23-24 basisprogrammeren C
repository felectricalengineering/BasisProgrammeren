/*

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

double oppervlakte_van_cirkel;
double omtrek_van_cirkel;
double straal_van_cirkel;

int main() {

printf("Geef de waarde van de straal van uw cirkel:\n\n");
scanf("%lf", &straal_van_cirkel);

oppervlakte_van_cirkel = M_PI * (straal_van_cirkel * straal_van_cirkel);
omtrek_van_cirkel = 2 * M_PI * straal_van_cirkel;

printf("\n\nDe oppervlakte van uw cirkel is %lf.\n", oppervlakte_van_cirkel);
printf("De omtrek van uw cirkel is %lf.\n", omtrek_van_cirkel);
}

*/

/*

#include <stdio.h>
#include <stdlib.h>

