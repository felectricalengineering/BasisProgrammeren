/*

#include <stdio.h>
#include <stdlib.h>

int eerste_getal;
int tweede_getal;
int derde_getal;

int main(){

printf("Kies een eerste geheel getal.\n\n");
scanf_s("%d", &eerste_getal);

printf("\n\nKies een tweede geheel getal.\n\n");
scanf_s("%d", &tweede_getal);

printf("\n\nKies een derde geheel getal.\n\n");
scanf_s("%d", &derde_getal);

if (eerste_getal > tweede_getal && eerste_getal > derde_getal){
    printf("\n\nHet grootste gehele getal is %d", eerste_getal);
}

else if (tweede_getal > eerste_getal && tweede_getal > derde_getal){
    printf("\n\nHet grootste gehele getal is %d", tweede_getal);
}

else {
    printf("\n\nHet grootste gehele getal is %d", derde_getal);
}
}

*/

